{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.29/schema.json",
  "app_specific_configuration_path": "$Env:KOMOREBI_CONFIG_HOME/applications.yaml",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_container_padding": 3,
  "border": true,
  "border_width": 3,
  "border_offset": -1,
  "mouse_follows_focus": true,
  "theme": {
    "palette": "Base16",
    "name": "Ashes",
    "single_border": "Base0B",
    "unfocused_border": "Base0E"
  },
  "stackbar": {
    "height": 20,
    "mode": "OnStack",
    "tabs": {
      "width": 300
    }
  },
  "monitors": [
    {
      "workspaces": [
        {
          "name": "COCKPIT",
          "layout": "Grid"
        },
        {
          "name": "SOCIAL",
          "layout": "Grid"
        },
        {
          "name": "SUSE",
          "layout": "Grid"
        },
        {
          "name": "WRITING",
          "layout": "Grid"
        },
        {
          "name": "V",
          "layout": "Grid"
        },
        {
          "name": "VI",
          "layout": "Grid"
        },
        {
          "name": "VII",
          "layout": "Grid"
        },
        {
          "name": "VIII",
          "layout": "Grid"
        },
        {
          "name": "IX",
          "layout": "Grid"
        }
      ]
    }
  ]
}
